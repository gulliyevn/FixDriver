{
  "name": "fixdrive",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo build:web",
    "eject": "expo eject",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:ci": "jest --config jest.config.js --ci --coverage --watchAll=false",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "ci": "node scripts/code-quality.js && node scripts/security-audit.js && node scripts/performance-check.js",
    "ci:full": "npm run type-check && npm run lint && npm run format:check && npm run test:ci && npm run ci",
    "pre-commit": "npm run type-check && npm run lint && npm run format:check",
    "setup-cicd": "bash scripts/setup-cicd.sh",
    "clean": "rm -rf node_modules/.cache && rm -rf .expo && rm -rf build && rm -rf dist",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "analyze": "npx expo export --platform web && npx webpack-bundle-analyzer dist/static/js/*.js",
    "logs": "npx expo logs",
    "doctor": "npx expo doctor",
    "install:fix": "npx expo install --fix",
    "prebuild": "npx expo prebuild",
    "postbuild": "npx expo postbuild"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@mapbox/polyline": "^1.2.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-native/virtualized-lists": "^0.80.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.4.9",
    "@testing-library/react-native": "^13.3.3",
    "expo": "54.0.13",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.12",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "formdata-polyfill": "^4.0.10",
    "i18n-js": "^4.5.1",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-worklets": "0.5.1",
    "semver": "^7.7.2",
    "webidl-conversions": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.5",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "audit-ci": "^6.0.0",
    "eslint": "^8.49.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.5.0",
    "jest-expo": "~54.0.12",
    "prettier": "^3.0.3",
    "react-test-renderer": "^19.1.0",
    "typescript": "^5.1.3"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/fixdrive.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "taxi",
    "ride-sharing",
    "mobile-app"
  ],
